@model System.Collections.Generic.List<contact_management.web.Models.ApplicationUser>

@{
  ViewBag.Title = "Active Users";
}

<h2>Active System Users</h2>

<table class="table">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>User Name</th>
    </tr>
  </thead>
  <tbody>
    @foreach (var user in Model)
    {
      <tr>
        <td>@user.FirstName</td>
        <td>@user.LastName</td>
        <td>@user.UserName</td>
      </tr>
    }
  </tbody>
</table>


